<!-- STUB graph -->
<div class="container-fluid"></div>
<div class="graph">

    <!-- STUB months -->
    <ul class="months">
        <li>Nov-20</li>
        <li>Dec-20</li>
        <li>Jan-21</li>
        <li>Feb-21</li>
        <li>Mar-21</li>
        <li>Apr-21</li>
        <li>May-21</li>
        <li>Jun-21</li>
        <li>Jul-21</li>
        <li>Aug-21</li>
        <li>Sep-21</li>
        <li>Oct-21</li>
    </ul>

    <!-- STUB Days -->
    <ul class="days">
        <li>Sun</li>
        <li>Mon</li>
        <li>Tue</li>
        <li>Wed</li>
        <li>Thu</li>
        <li>Fri</li>
        <li>Sat</li>
    </ul>

    <!-- STUB squares -->
    <ul class="squares">
        <% for (var i = 0; i < 364; i++) { %>
            <li data-toggle="tooltip" 
                data-placement="bottom" 
                data-animation="false" 
                delay="0" 
                title="<%= data[1][i] %>" 
                data-level="<%= data[3][i] %>">
            </li>
        <% } %>

    </ul>
</div>
</div>